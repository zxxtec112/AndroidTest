<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_register"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.administrator.zyfypt_421lff.RegisterActivity">

    <TextView
        android:text=" "
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView8"
        android:background="@drawable/lff2" />

    <TextView
        android:text="Amicool 注册"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="33dp"
        android:id="@+id/textView9"
        android:textSize="30sp"
       />

    <TextView
        android:text="请先填写注册信息"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginStart="28dp"
        android:layout_marginTop="15dp"
        android:id="@+id/textView12"

        android:textSize="18sp" />

    <TextView
        android:text="用  户  名："
        android:layout_below="@+id/textView12"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="12dp"
        android:id="@+id/textView13"
        android:textColor="?android:attr/textColorHighlight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp" />

    <TextView
        android:text="密        码："
        android:layout_width="wrap_content"
        android:id="@+id/textView14"

        android:textSize="18sp"
        android:layout_alignStart="@+id/textView11"
        android:layout_height="wrap_content"
        android:textColor="?android:attr/textColorHighlight"
        android:layout_below="@+id/editText" />

    <TextView
        android:text="密码确认："
        android:layout_width="wrap_content"
        android:layout_below="@+id/textView14"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="15dp"
        android:id="@+id/textView15"

        android:textSize="18sp"
        android:layout_height="wrap_content"
        android:textColor="?android:attr/textColorHighlight" />

    <TextView
        android:text="真实姓名："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView15"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="20dp"
        android:id="@+id/textView16"
        android:textSize="18sp"
        android:textColor="?android:attr/textColorHighlight" />

    <TextView
        android:text="年        龄："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView16"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="15dp"
        android:id="@+id/textView17"

        android:textSize="18sp"
        android:textColor="?android:attr/textColorHighlight" />

    <TextView
        android:text="头        像："
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView17"
        android:layout_alignStart="@+id/textView11"
        android:layout_marginTop="18dp"
        android:id="@+id/textView18"
        android:textColor="?android:attr/textColorHighlight"
        android:textSize="18sp" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" "
        android:ems="10"
        android:id="@+id/editText"
        android:layout_alignBaseline="@+id/textView13"
        android:layout_alignBottom="@+id/textView13"
        android:layout_toEndOf="@+id/textView13"
        android:hint="用户名"
        android:inputType="textPersonName" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:ems="10"
        android:id="@+id/editText2"
        android:layout_alignBaseline="@+id/textView14"
        android:layout_alignBottom="@+id/textView14"
        android:layout_toEndOf="@+id/textView14"
        android:hint="密码" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberPassword"
        android:ems="10"
        android:id="@+id/editText3"
        android:layout_alignBaseline="@+id/textView15"
        android:layout_alignBottom="@+id/textView15"
        android:layout_toEndOf="@+id/textView15"
        android:hint="密码确认" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:text=" "
        android:ems="10"
        android:id="@+id/editText4"
        android:layout_below="@+id/editText3"
        android:layout_toEndOf="@+id/textView16"
        android:hint=" " />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/editText5"
        android:hint="年龄"
        android:layout_alignBaseline="@+id/textView17"
        android:layout_alignBottom="@+id/textView17"
        android:layout_toEndOf="@+id/textView17" />

    <Button
        android:text="点击拍照"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button3"
        android:layout_alignBaseline="@+id/textView18"
        android:layout_alignBottom="@+id/textView18"
        android:layout_centerHorizontal="true" />

    <Button
        android:text="注册"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView11"
        android:layout_alignStart="@+id/textView9"
        android:layout_marginBottom="24dp"
        android:id="@+id/button4" />

    <Button
        android:text="返回登录"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/button4"
        android:layout_toEndOf="@+id/textView12"
        android:layout_marginStart="20dp"
        android:id="@+id/button5" />

    <TextView
        android:text=" "
        android:layout_width="320dp"
        android:layout_height="380dp"
        android:layout_marginTop="14dp"
        android:id="@+id/textView11"
        android:background="?attr/colorControlHighlight"
        android:layout_below="@+id/textView9"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
